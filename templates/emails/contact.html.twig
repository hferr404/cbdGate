        {% endblock %}

   <table class="table table-bordered text-center text-white bg-secondary col-md-10 mx-auto">
    <tr>
        <td>Prénom</td>
        <td>{{ contact.prenom }}</td>
    </tr>
    <tr>
        <td>Nom</td>
        <td>{{ contact.nom }}</td>
    </tr>
    <tr>
        <td>Téléphone</td>
        <td>{{ contact.telephone }}</td>
    </tr>
    <tr>
        <td>Email</td>
        <td>{{ contact.email }}</td>
    </tr>
    <tr>
        <td>Message</td>
        <td>{{ contact.message }}</td>
    </tr>
</table>


